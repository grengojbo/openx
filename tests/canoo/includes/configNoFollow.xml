<!-- canoo configuration passing an option to HTTPUnit to not follow redirects -->
<config
  host="${host}"
  port="${port}"
  protocol="${protocol}"
  basepath="${basepath}"
  resultpath="target"
  resultfile="simpleResult.xml"
  summary="true"
  saveresponse="true" >
  <option name="RedirectEnabled" value="false" />
  <header name="followRedirects" value="false" />
</config>