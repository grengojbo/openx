<?xml version="1.0" encoding="ISO-8859-1" ?>
<?xml-stylesheet type="text/xsl" href=""?>

<plugin>
    <name>ox_core</name>
    <creationDate>2008-02-19</creationDate>
    <author>Radek Maciaszek</author>
    <authorEmail>radek.maciaszek@openx.org</authorEmail>
    <authorUrl>http://www.openx.org</authorUrl>
    <license>license.txt</license>
    <description>Plugin prepares data for buckets logging</description>
    <version>1.0.0-dev</version>
    <oxversion>2.7.5-dev</oxversion>
    <extends>deliveryDataPrepare</extends>

    <install>
        <files>
            <file path="{MODULEPATH}deliveryDataPrepare/ox_core/">ox_core.delivery.php</file>
        </files>

        <schema>
            <mdb2schema>tables_ox_core</mdb2schema>
            <dboschema>db_schema</dboschema>
            <dbolinks>db_schema.links</dbolinks>
            <dataobject>Data_bucket_click.php</dataobject>
            <dataobject>Data_bucket_fb_impression.php</dataobject>
            <dataobject>Data_bucket_impression.php</dataobject>
            <dataobject>Data_bucket_impression_country.php</dataobject>
            <dataobject>Data_bucket_request.php</dataobject>
        </schema>

    </install>

    <components>
       <component>
            <name>ox_core</name>
            <hook>preLog</hook>
       </component>
    </components>

</plugin>
