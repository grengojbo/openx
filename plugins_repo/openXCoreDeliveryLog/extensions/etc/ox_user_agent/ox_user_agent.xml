<?xml version="1.0" encoding="ISO-8859-1" ?>
<?xml-stylesheet type="text/xsl" href=""?>

<plugin>
    <name>ox_user_agent</name>
    <creationDate>2008-02-19</creationDate>
    <author>Radek Maciaszek</author>
    <authorEmail>radek.maciaszek@openx.org</authorEmail>
    <authorUrl>http://www.openx.org</authorUrl>
    <license>license.txt</license>
    <description>Plugin prepares user agent data for buckets logging</description>
    <version>1.0.0-dev</version>
    <oxversion>2.7.5-dev</oxversion>
    <extends>deliveryDataPrepare</extends>

    <install>
        <files>
            <file path="{MODULEPATH}deliveryDataPrepare/ox_user_agent/">ox_user_agent.delivery.php</file>
        </files>

        <components>
           <component>
                <name>ox_user_agent</name>
                <hook>preLog</hook>
           </component>
        </components>

    </install>

</plugin>
