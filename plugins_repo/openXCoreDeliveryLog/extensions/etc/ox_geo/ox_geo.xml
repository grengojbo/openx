<plugin>
    <name>ox_geo</name>
    <creationDate>2008-02-19</creationDate>
    <author>Radek Maciaszek</author>
    <authorEmail>radek.maciaszek@openx.org</authorEmail>
    <authorUrl>http://www.openx.org</authorUrl>
    <license>license.txt</license>
    <description>Plugin prepares geotargeting data for buckets logging</description>
    <version>1.0.0-dev</version>
    <oxversion>2.7.5-dev</oxversion>
    <extends>deliveryDataPrepare</extends>

    <install>
        <files>
            <file path="{MODULEPATH}deliveryDataPrepare/ox_geo/">ox_geo.delivery.php</file>
        </files>
    </install>

    <register>
        <hook type='delivery' hook="preLog"></hook>
    </register>

</plugin>
