<?xml version="1.0" encoding="ISO-8859-1" ?>
<?xml-stylesheet type="text/xsl" href=""?>

<plugin>
    <name>openXCoreDeliveryLog</name>
    <creationDate>2008-06-10</creationDate>
    <author>Radek Maciaszek</author>
    <authorEmail>radek.maciaszek@openx.org</authorEmail>
    <authorUrl>http://www.openx.org</authorUrl>
    <license>license.txt</license>
    <description>Core Delivery Logging Plugins for OpenX</description>
    <version>1.0.0-dev</version>
    <type>extension</type>

    <install>

        <!--files>
            <file path="{PLUGINPATH}">openXDeliveryLog.readme.txt</file>
        </files-->

        <contents>
            <group name="oxDeliveryDataPrepare">1</group>

            <group name="oxLogClick"           >2</group>
            <group name="oxLogImpression"      >3</group>
            <group name="oxLogRequest"         >4</group>
        </contents>

        <register>
            <extension type='deliveryDataPrepare' group='oxDeliveryDataPrepare'>Prepares data for delivery logging</extension>

            <extension type='deliveryLog' group='oxLogClick'>Logs clicks</extension>
            <extension type='deliveryLog' group='oxLogImpression'>Logs impressions</extension>
            <extension type='deliveryLog' group='oxLogRequest'>Logs requests</extension>
        </register>

    </install>

    <readme>

    </readme>

</plugin>
